include utils
doctype html

html
    head
        // Shared header resources
        - css("/static/app/css/style.css");
        - script("/static/app/js/chunk-vendors.js");
        - css("/static/app/css/chunk-vendors.css");
        - css("/static/app/css/app.css");
        - css("https://unpkg.com/spectre.css/dist/spectre.min.css");

        meta(name="viewport", content="width=device-width, initial-scale=1.0")
        meta(charset="utf-8")

        link(rel="shortcut icon", href="/static/favicon.png")
        link(rel="apple-touch-icon-precomposed", href="/static/favicon.png")


    body
        #app
        
        - script("/static/app/js/app.js");
